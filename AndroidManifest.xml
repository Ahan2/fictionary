<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ahan.sabharwal.fictionary"
    android:versionCode="1"
    android:versionName="1.0" >
    
    

    <uses-sdk
        android:minSdkVersion= "11"
        android:targetSdkVersion="18" />
    
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon1"
        android:label="@string/app_name"
        android:theme="@style/MyTheme"
         >
        <activity
            android:name="com.ahan.sabharwal.fictionary.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/app"
            android:name="com.ahan.sabharwal.fictionary.Fake_word"
            android:parentActivityName="com.ahan.sabharwal.fictionary.MainActivity"
            android:screenOrientation="portrait" >
            
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.fictionary.MainActivity" />
        </activity>
    </application>
    
    

</manifest>